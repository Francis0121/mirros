<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="HomeMapper">

	<resultMap type="com.bg.jtown.business.Home" id="homeMap">
		<result column="username" property="username"/>
	</resultMap>

	<select id="getHomeTest" resultMap="homeMap">
		SELECT
			id
		FROM
			users
		WHERE
			pn = 1
	</select>
	
	<insert id="insertHomeTest" parameterType="com.bg.jtown.business.Home">
		INSERT 
		INTO
			test	(username)
		VALUES
			(#{username})
	</insert>
	
</mapper>